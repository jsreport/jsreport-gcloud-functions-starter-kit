{
    "httpPort": 5488,
    "trustUserCode": true,
    "reportTimeout": 60000,
    "workers": {
        "numberOfWorkers": 2
    },
    "store": {
        "provider": "fs"
    },
    "blobStorage": {
        "provider": "fs"
    },
    "logger": {
        "console": {
            "transport": "console",
            "level": "debug"
        },
        "file": {
            "transport": "file",
            "level": "info",
            "filename": "logs/reporter.log"
        },
        "error": {
            "transport": "file",
            "level": "error",
            "filename": "logs/error.log"
        }
    }
}